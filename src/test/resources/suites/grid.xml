<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="actiTIME Browser" parallel="tests" preserve-order="true">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="actiTIME - chrome Smoke Test">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.actitime.tests.web.Login"></class>
			<class name="com.actitime.tests.web.CreateUser"></class>
			<class name="com.actitime.tests.web.VerifyUser"></class>
			<class name="com.actitime.tests.web.DeleteUser"></class>
		</classes>
	</test>
	<test name="actiTIME - internet explorer Smoke Test">
		<parameter name="browser" value="internetexplorer" />
		<classes>
			<class name="com.actitime.tests.web.Login"></class>
			<class name="com.actitime.tests.web.CreateUser"></class>
			<class name="com.actitime.tests.web.VerifyUser"></class>
			<class name="com.actitime.tests.web.DeleteUser"></class>
		</classes>
	</test>
</suite>